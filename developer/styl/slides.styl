    display flex
    align-items stretch
    justify-content space-between
    .flag
        background red
        flex-grow 1
        line-height 600px
        height 600px
        width 200px
        text-align center
        position relative
        text-transform uppercase
        &.ua
            background #fff url('../images/ua.jpg') 0 0 no-repeat
            background-size 800px 600px
        &.ru
            background #fff url('../images/ru.jpg') -200px 0 no-repeat
            background-size 800px 600px
        &.kz
            background #fff url('../images/kz.jpg') -400px 0 no-repeat
            background-size 800px 600px
        &.by
            background #fff url('../images/by.jpg') -600px 0 no-repeat
            background-size 800px 600px

