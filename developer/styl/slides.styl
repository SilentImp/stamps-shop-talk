article.title
    background #fff url('../images/title.jpg') 50% 50% no-repeat
    background-size cover

article.domain
    background #fff url('../images/domain.jpg') 50% 50% no-repeat
    background-size cover
    h1
        height 300px
        width 300px
        line-height 300px
        font-size 64px
        left 50%
        top 50%
        transform translateX(-50%) translateY(-50%)
        &:after
            display none

article.tld
    display flex
    align-items stretch
    justify-content space-between
    .tlds
        background red
        flex-grow 1
        line-height 600px
        text-align center
        position relative
        text-transform uppercase
        &.tk
            background #fff url('../images/tk.jpg') 50% 50% no-repeat
            background-size cover
        &.ml
            background #fff url('../images/ml.jpg') 50% 50% no-repeat
            background-size cover
        &.ga
            background #fff url('../images/ga.jpg') 50% 50% no-repeat
            background-size cover
        &.cf
            background #fff url('../images/cf.jpg') 50% 50% no-repeat
            background-size cover
        &.gq
            background #fff url('../images/gq.jpg') 50% 50% no-repeat
            background-size cover
        span
            display inline-block
            border-radius 100%
            width 100px
            height 100px
            line-height 100px
            font-size 36px
            background-color #fff
            left 50%
            top 50%
            transform translateX(-50%) translateY(-50%)
            position absolute

